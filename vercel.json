{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/signed-url",
      "dest": "/api/signed-url"
    },
    {
      "src": "/api/check-balance",
      "dest": "/api/check-balance"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
